{
    "author annotation":
    {
        "prefix": ["aann"],
        "body":
        [
            "$LINE_COMMENT--------------------------------------------------------------------------- ",
            "$LINE_COMMENT Author : J. McAuley Penney ",
            "$LINE_COMMENT Purpose: $1",
            "$LINE_COMMENT Process:",
            "$LINE_COMMENT Notes  :",
            "$LINE_COMMENT--------------------------------------------------------------------------- "
        ],
        "description": "Annotation discussing file info"
    },

    "function template":
    {
        "prefix": ["fann"],
        "body":
        [
            "$LINE_COMMENT--------------------------------------------------------------------------- ",
            "$LINE_COMMENT Name   : $1",
            "$LINE_COMMENT Context:",
            "$LINE_COMMENT Process:",
            "$LINE_COMMENT Params :",
            "$LINE_COMMENT Output :",
            "$LINE_COMMENT Notes  :",
            "$LINE_COMMENT Docs   :",
            "$LINE_COMMENT--------------------------------------------------------------------------- "
        ],
        "description": "Annotation discussing function info"
    },

    "function annotation":
    {
        "prefix": ["function template"],
        "body":
        [
            "$LINE_COMMENT--------------------------------------------------------------------------- ",
            "$LINE_COMMENT Name   :",
            "$LINE_COMMENT Context:",
            "$LINE_COMMENT Process:",
            "$LINE_COMMENT Params :",
            "$LINE_COMMENT Output :",
            "$LINE_COMMENT Notes  :",
            "$LINE_COMMENT Docs   :",
            "$LINE_COMMENT--------------------------------------------------------------------------- ",
            "def $1():",
            "    pass"
        ],
        "description": "Generic function template"
    },

    "parameter annotation":
    {
        "prefix": ["pann"],
        "body":
        [
            "$LINE_COMMENT",
            "$LINE_COMMENT          $1",
            "$LINE_COMMENT           - type: ",
            "$LINE_COMMENT           - desc: ",
            "$LINE_COMMENT"
        ],
        "description": "\"Function Annotation\" \"Parameter\" field template"
    },

    "Other Docs annotation":
    {
        "prefix": ["dann"],
        "body":
        [
            "$LINE_COMMENT                 - topic : $1",
            "$LINE_COMMENT                   - link: "
        ],
        "description": "\"Function Annotation\" \"Other Docs\" field template"
    }
}
