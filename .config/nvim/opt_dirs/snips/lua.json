{
    "basic annotation": 
    {
        "prefix": ["ann"],
        "body": 
        [
            "--[[",
            "    $1",
            "]]"
        ],                 
        "description": "General annotation template"
    },  

    "author annotation": 
    {
        "prefix": ["aann"],
        "body": 
        [
            "--[[",
            "    @author: Jacob Penney",
            "    @since :",
            "]]"
        ],
        "description": "Annotation discussing file info"
    },

    "function annotation":
    {
        "prefix": ["fann"],
        "body": 
        [
            "--[[",
            "    @name  :$1", 
            "    @param :", 
            "    @return:",
            "]]"
        ],
        "description": "Annotation discussing function info"
    }
} 
